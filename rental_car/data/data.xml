<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="seq_salik_car_transaction" model="ir.sequence">
            <field name="name">Salik Car Transaction</field>
            <field name="code">salik.car.transaction</field>
            <field name="prefix">Salik-No/</field>
            <field name="padding">5</field>
        </record>


        <record id="seq_fines_cars" model="ir.sequence">
            <field name="name">Car Fines</field>
            <field name="code">car.fines</field>
            <field name="prefix">Fine-No/</field>
            <field name="padding">5</field>
        </record>

        <record forcecreate="True" id="product_product_salik_service_id" model="product.product">
            <field name="name">salik service product</field>
            <field name="detailed_type">service</field>
            <field name="standard_price">0.0</field>
            <field name="list_price">2.0</field>
        </record>
        <record forcecreate="True" id="product_product_salik_expense_id" model="product.product">
            <field name="name">salik expense product</field>
            <field name="detailed_type">service</field>
            <field name="standard_price">0.0</field>
            <field name="list_price">4.0</field>
        </record>

<!--                                            Fines                                                                   -->


         <record forcecreate="True" id="product_product_fines_expense_id" model="product.product">
            <field name="name">Fine Expense product</field>
            <field name="detailed_type">service</field>
<!--            <field name="standard_price">0.0</field>-->
<!--            <field name="list_price">0.0</field>-->
        </record>


        <record forcecreate="True" id="product_product_fines_surcharges" model="product.product">
            <field name="name">Fine Surcharges</field>
            <field name="detailed_type">service</field>
<!--            <field name="standard_price">0.0</field>-->
<!--            <field name="list_price">0.0</field>-->
        </record>


    </data>
</odoo>